import numpy as np
from matplotlib import pyplot as plt
import sympy as sp

HR = 60
tc = 60/HR
Tmax = 0.2+0.15*tc
t = np.arange(0, 1, 0.01)
tn = t / Tmax

def elastance(tn):
    Emax = 2
    Emin = 0.06
    En = 1.55 * (((tn / 0.7) ** 1.9) / (1 + ((tn / 0.7) ** 1.9))) * (1 / (1 + ((tn / 1.17) ** 21.9)))
    E = (Emax-Emin)*En+Emin
    return E

E =
#x = sp.Symbol('tn')
Eprime = np.diff(elastance(tn))

print(Eprime)

plt.plot(t,elastance(tn))
#plt.plot(t,Eprime)
plt.xlabel('time [s]')
plt.ylabel('Elastance [mmHg/mL]')
plt.show()

